<div class="admin-section admin-section2">
  <div class="admin-section-panel admin-section-panel4">

    <section class="account-info-container">
      <h1>Transaction History Detail</h1>
        <h3>{{data.username}}</h3>
        <div class="account-container">
      {{#if isWithdraw}}
      <table>
            <tr>
                <th>Mã giao dịch</th>
                <td>{{data.id}}</td>
                <th>Số thẻ</th>
                <td>{{data.card_number}}</td>
            </tr>
            <tr>
                <th>Ngày rút tiền</th>
                <td>{{data.date}}</td>
                <th>Số tiền rút</th>
                <td>{{data.value}}</td>
            </tr>
            <tr>
                <th>Phí giao dịch</th>
                <td>{{data.fee}}</td>
                <th>Status</th>
                {{{data.status}}}
            </tr>
            <tr>
                <th>Ghi chú</th>
                <td>{{data.note}}</td>
            </tr>
      </table>
      {{/if}}
      {{#if isRecharge}}
      <table>
            <tr>
                <th>Mã giao dịch</th>
                <td>{{data.id}}</td>
                <th>Số thẻ</th>
                <td>{{data.card_number}}</td>
            </tr>
            <tr>
                <th>Ngày nạp tiền</th>
                <td>{{data.date}}</td>
                <th>Số tiền nạp</th>
                <td>{{data.value}}</td>
            </tr>
            <tr>
               <th>Trạng thái</th>
                {{{data.status}}}
            </tr>
      </table>
      {{/if}}
      {{#if isDeposit}}
      <table>
            <tr>
                <th>Mã giao dịch</th>
                <td>{{data.id}}</td>
            </tr>
            <tr>
                <th>Số điện thoại chuyển tiền</th>
                <td>{{data.phone_sender}}</td>
                <th>Số điện thoại nhận tiền</th>
                <td>{{data.phone_receiver}}</td>
            </tr>
            <tr>
                <th>Số tiền chuyển</th>
                <td>{{data.value}}</td>
                <th>Phí giao dịch</th>
                <td>{{data.fee}}</td>
            </tr>
            <tr>
                <th>Người chịu phí</th>
                <td>{{data.feeperson}}</td>
                <th>Ghi chú</th>
                <td>{{data.note}}</td>
            </tr>
            <tr>
                <th>Ngày chuyển tiền</th>
                <td>{{data.date}}</td>
                <th>Trạng thái</th>
                {{{data.status}}}
            </tr>
      </table>
      {{/if}}

      {{#if isBill}}
      <table>
            <tr>
                <th>Mã giao dịch</th>
                <td>{{data.id}}</td>
            </tr>
            <tr>
                <th>Mã số nhà mạng</th>
                <td>{{data.provider_number}}</td>
                <th>Các loại mã thẻ đã mua</th>
                <td>{{data.code}}</td>
            </tr>
            <tr>
                <th>Số lượng mua</th>
                <td>{{data.quantity}}</td>
                <th>Tổng tiền</th>
                <td>{{data.price}}</td>
            </tr>
            <tr>
                <th>Ngày chuyển tiền</th>
                <td>{{data.date}}</td>
                <th>Trạng thái</th>
                {{{data.status}}}
            </tr>
      </table>
      {{/if}}
    </section>
  </div>
</div>